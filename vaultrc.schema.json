{
  "$schema": "http://json-schema.org/draft-07/schema",
  "properties": {
    "url": {
      "default": "//vault.json.soy",
      "type": "string",
      "format": "url"
    },
    "editor": {
      "default": "vim",
      "oneOf": [
        {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        {
          "type": "string",
          "enum": ["vim", "vscode"]
        }
      ]
    }
  }
}
